{
  "name": "Dave",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Removes AI features from Search Engines.",
  "permissions": ["declarativeNetRequest", "scripting"],
  "host_permissions": ["<all_urls>"],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "content_scripts": [
    {
      "matches": ["https://www.bing.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}